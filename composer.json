{
    "name": "dachcom-digital/pimcore-formbuilder",
    "type": "pimcore-plugin",
    "license": "GPL-3.0+",
    "description": "Pimcore 4.0 Build Forms quickly and easily",
    "keywords": ["pimcore", "formbuilder", "form"],
    "homepage": "https://github.com/dachcom-digital/pimcore-formbuilder",
    "authors": [
       {
            "name": "Stefan Hagspiel",
            "homepage": "http://www.dachcom-digital.ch",
            "role": "Developer"
       }
    ],

    "require": {
        "pimcore/installer-plugin": ">=1",
        "cgsmith/zf1-recaptcha-2": "dev-master"
    },

    "repositories": [
        {
        "type": "package",
        "package": {
            "name": "cgsmith/zf1-recaptcha-2",
            "version": "dev-master",
            "source": {
                "url": "https://github.com/solverat/zf1-recaptcha-2.git",
                "type": "git",
                "reference": "dev-solverat"
            },
            "autoload": {
                "classmap": [""]
            }
        }
        }
    ]
}
